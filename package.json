    "@types/material-ui": "0.21.5",
    "@types/node": "10.12.0",
    "@types/raven": "2.5.1",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-i18next": "7.8.3",
    "@types/react-router": "4.0.32",
    "cypress": "3.1.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "husky": "1.1.2",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "typescript": "3.1.3"
  }
