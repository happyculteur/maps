    "@material-ui/styles": "4.3.0",
    "@reach/router": "1.2.1",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.3",
    "@types/i18next": "12.1.0",
    "@types/i18next-browser-languagedetector": "3.0.0",
    "@types/jest": "24.0.16",
    "@types/leaflet": "1.5.0",
    "@types/lodash": "4.14.136",
    "@types/material-ui": "0.21.6",
    "@types/node": "12.6.9",
    "@types/raven": "2.5.3",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-helmet": "5.0.8",
    "@types/uuid": "3.4.5",
    "classnames": "2.2.6",
    "i18next": "17.0.6",
    "commitizen": "4.0.3",
    "i18next-browser-languagedetector": "3.0.1",
    "leaflet": "1.5.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "raven-js": "3.27.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-i18next": "10.11.5",
    "react-markdown": "4.1.0",
    "react-scripts": "3.3.0",
    "react-spinners": "0.5.13",
    "react-toastify": "5.3.2",
    "typescript": "3.5.3",
    "uuid": "3.3.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "ci": "start-server-and-test start http-get://localhost:3000 cypress:run",
    "clean": "rm -rf node_modules",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test start http-get://localhost:3000 cypress:open",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "tslint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test-with-coverage": "react-scripts test --env=jsdom --coverage && cat ./coverage/lcov.info | codacy-coverage",
    "test:debug": "react-scripts --inspect-brk test --env=jsdom --runInBand --no-cache --no-watch "
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/**/index.ts",
      "!src/**/registerServiceWorker.ts",
      "!src/**/*.e2e.{ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "post-checkout": "npm i && npm outdated",
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && npx git-cz --hook"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "codacy-coverage": "3.4.0",
    "cypress": "3.4.1",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint-plugin-react-hooks": "1.6.1",
    "husky": "3.0.2",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "start-server-and-test": "1.9.1",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
